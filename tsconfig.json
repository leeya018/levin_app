{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "babel.config.js",
    "tailwind.config.js",
    "app/(tabs)/shifts.tsx",
    "app/(tabs)/procedures.tsx",
    "app/(tabs)/index.tsx",
    "app/(tabs)/_layout.tsx_"
  ]
}
